{
  "openapi": "3.0.1",
  "info": {
    "title": "OncoWeb API",
    "version": "v1"
  },
  "paths": {
    "/api/admin/ingest": {
      "post": {
        "tags": [
          "Admin"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/admin/ingest/single": {
      "post": {
        "tags": [
          "Admin"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestJob"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestJob"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/IngestJob"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/genes/{cohort}/{patientId}": {
      "get": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "patientId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/GeneExpressionDoc"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GeneExpressionDoc"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/GeneExpressionDoc"
                }
              }
            }
          }
        }
      }
    },
    "/api/genes/query": {
      "post": {
        "tags": [
          "Genes"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GeneQuery"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/GeneQuery"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/GeneQuery"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GeneExpressionDoc": {
        "type": "object",
        "properties": {
          "id": {
            "$ref": "#/components/schemas/ObjectId"
          },
          "patientId": {
            "type": "string",
            "nullable": true
          },
          "cancerCohort": {
            "type": "string",
            "nullable": true
          },
          "expressions": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          },
          "ingestedAtUtc": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "GeneQuery": {
        "type": "object",
        "properties": {
          "cohort": {
            "type": "string",
            "nullable": true
          },
          "patients": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "genes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IngestJob": {
        "type": "object",
        "properties": {
          "cohort": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "objectName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IngestRequest": {
        "type": "object",
        "properties": {
          "jobs": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/IngestJob"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ObjectId": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "creationTime": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}