{
  "openapi": "3.0.1",
  "info": {
    "title": "OncoWeb API",
    "version": "v1"
  },
  "paths": {
    "/api/admin/ingest": {
      "post": {
        "tags": [
          "Admin"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/admin/ingest/local": {
      "post": {
        "tags": [
          "Admin"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cohort": {
                    "type": "string"
                  },
                  "File": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Cohort": {
                  "style": "form"
                },
                "File": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/genes/import": {
      "post": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "objectName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/genes/{cohort}/{patientId}": {
      "get": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "patientId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/GeneExpressionDoc"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GeneExpressionDoc"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/GeneExpressionDoc"
                }
              }
            }
          }
        }
      }
    },
    "/api/genes/by-ids": {
      "post": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "text/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "application/*+json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/GeneExpressionDoc"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/GeneExpressionDoc"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/GeneExpressionDoc"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GeneExpressionDoc": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "patientId": {
            "type": "string",
            "nullable": true
          },
          "cancerCohort": {
            "type": "string",
            "nullable": true
          },
          "genes": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IngestJob": {
        "type": "object",
        "properties": {
          "cohort": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "objectName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IngestRequest": {
        "type": "object",
        "properties": {
          "jobs": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/IngestJob"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}