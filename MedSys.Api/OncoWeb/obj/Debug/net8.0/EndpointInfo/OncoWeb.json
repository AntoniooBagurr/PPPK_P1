{
  "openapi": "3.0.1",
  "info": {
    "title": "OncoWeb API",
    "version": "v1"
  },
  "paths": {
    "/api/admin/ingest": {
      "post": {
        "tags": [
          "Admin"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/IngestRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/admin/ingest/local": {
      "post": {
        "tags": [
          "Admin"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cohort": {
                    "type": "string"
                  },
                  "File": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Cohort": {
                  "style": "form"
                },
                "File": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/clinical/import": {
      "post": {
        "tags": [
          "Clinical"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "objectName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/clinical/{cohort}/{patientId}": {
      "get": {
        "tags": [
          "Clinical"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "patientId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ClinicalDoc"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClinicalDoc"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ClinicalDoc"
                }
              }
            }
          }
        }
      }
    },
    "/api/genes/{cohort}/patients": {
      "get": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "prefix",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "skip",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 0
            }
          },
          {
            "name": "take",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 50
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/genes/import": {
      "post": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "objectName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/genes/{cohort}/panel/{patientId}": {
      "get": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "patientId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/genes/{cohort}/gene/{gene}/top": {
      "get": {
        "tags": [
          "Genes"
        ],
        "parameters": [
          {
            "name": "cohort",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "gene",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 20
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": { }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": { }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ClinicalDoc": {
        "type": "object",
        "properties": {
          "id": {
            "$ref": "#/components/schemas/ObjectId"
          },
          "patientId": {
            "type": "string",
            "nullable": true
          },
          "cancerCohort": {
            "type": "string",
            "nullable": true
          },
          "dss": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "os": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "clinicalStage": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IngestJob": {
        "type": "object",
        "properties": {
          "cohort": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "objectName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IngestRequest": {
        "type": "object",
        "properties": {
          "jobs": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/IngestJob"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ObjectId": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "creationTime": {
            "type": "string",
            "format": "date-time",
            "readOnly": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}